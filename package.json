    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "remix": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
