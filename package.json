  "scripts": {
    "postinstall": "remix setup node",
    "dev": "cross-env NODE_ENV=development concurrently \"npm:dev:*\"",
    "dev:functions": "cd server && tsc --watch",
    "dev:styles": "postcss styles --base styles --dir app/styles -w",
    "dev:firebase": "firebase emulators:start",
    "dev:remix": "remix watch",
    "build": "cross-env NODE_ENV=production npm run build:styles && remix build && npm run build:functions",
    "build:functions": "cd server && tsc",
    "build:styles": "postcss styles --base styles --dir app/styles --env production"
  },
  "dependencies": {
    "@remix-run/express": "^1.0.4",
    "@remix-run/react": "^1.0.4",
    "express": "^4.17.1",
    "firebase-admin": "^10.0.0",
    "firebase-functions": "^3.16.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.4",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "firebase-tools": "^9.23.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": "16"
  },
  "main": "server/build/index.js",
  "sideEffects": false
}
